{
  "name": "ultimate-nest-graphql-backend",
  "version": "0.0.1",
  "description": "",
  "author": {
    "name": "Greg Teixeira",
    "email": "greg.teixeira123@gmail.com",
    "url": "https://github.com/greguintow"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "env-cmd --silent nest start --watch --watchAssets",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "env-cmd -f .env.test jest --verbose --runInBand",
    "test:watch": "env-cmd -f .env.test jest --verbose --watch",
    "test:cov": "env-cmd -f .env.test jest --verbose --coverage",
    "test:debug": "env-cmd -f .env.test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "yarn test:docker:up && wait-on tcp:27016 && env-cmd -f .env.test jest --config ./test/jest-e2e.config.ts --verbose && yarn test:docker:down",
    "test:docker:up": "docker-compose -f docker-compose-test.yml up -d",
    "test:docker:down": "docker-compose -f docker-compose-test.yml down",
    "dev:docker:up": "docker-compose -f docker-compose-dev.yml up -d",
    "postinstall": "husky install || true",
    "init:dev": "docker-compose up -d db && cp .env.dev .env",
    "seed:local:load": "env-cmd -f .env.test nestjs-command create:user && env-cmd -f .env.test nestjs-command create:acl"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^2.18.4",
    "@grpc/grpc-js": "^1.4.5",
    "@nestjs/common": "^8.2.3",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.3",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.2.3",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/terminus": "^8.0.1",
    "@ntegral/nestjs-sentry": "^3.0.7",
    "@sentry/node": "^6.16.1",
    "@sentry/types": "^6.16.1",
    "class-transformer": "^0.5.1",
    "class-validator": "npm:@greguintow/class-validator@0.13.7",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.1.0",
    "nestjs-command": "^3.1.1",
    "newrelic": "^8.7.0",
    "query-string": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.2.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@golevelup/nestjs-testing": "^0.1.2",
    "@nestjs/cli": "^8.1.5",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.3",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@types/jest-when": "^2.7.3",
    "@types/newrelic": "^7.0.3",
    "@types/node": "^16.11.10",
    "@types/supertest": "^2.0.11",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^7.0.0",
    "jest": "27.3.1",
    "jest-when": "^3.4.2",
    "lint-staged": "^12.1.2",
    "nock": "^13.2.1",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2",
    "wait-on": "^6.0.0"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}
